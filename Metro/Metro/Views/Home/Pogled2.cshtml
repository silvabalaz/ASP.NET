@model Metro.Models.Mapa

@using Metro.Models



@{
    var m = TempData["M"] as Mapa;
}

<b>Duljina trazene rute:</b>@ViewBag.Message


<table>
      <th>Ime Metroa: @m.ImeMetroa</th>
    <tr>
        <th>Start Rute</th>
        <th>Kraj Rute</th>
        <th>Duljina Rute</th>
    </tr>
    @{ var Lista = m.KonstrukcijaRuta();}
    
     @foreach (Ruta r in Lista) { 
      <tr>   
      <td>From @r.Start.KvartIme</td>
      <td>to @r.Kraj.KvartIme </td>
      <td>-> @r.Duljina </td> 
 
   </tr>}
     
</table>

<table>
      <th>Ime Metroa: @m.ImeMetroa</th>
